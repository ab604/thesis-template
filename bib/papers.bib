  keywords        = {Animals; Antibodies, Monoclonal, classification; Antibody Specificity; Antigens, Heterophile, genetics; Binding Sites, Antibody; Cell Line; Cross Reactions; Enzyme-Linked Immunosorbent Assay; Genes, MHC Class II; Genotype; HLA Antigens, classification, genetics, immunology; Haploidy; Histocompatibility Antigens Class II, classification, genetics, immunology; Humans; Lymphocytes, Null, immunology; Mice; Mutation},
  nlm-id          = {8010936},
