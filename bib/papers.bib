  issue           = {4},
  keywords        = {Antigen Presentation, immunology; Cell Division, immunology; Coculture Techniques; Dermatitis, Contact, immunology; HeLa Cells; Histocompatibility Antigens Class II, immunology; Humans; Keratinocytes, immunology; Receptors, Cell Surface, immunology; T-Lymphocytes, immunology; Th1 Cells, immunology},
  nlm-id          = {0323767},
