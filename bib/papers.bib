@Article{parham1981,
  author          = {Parham, P and Brodsky, F M},
  title           = {Partial purification and some properties of BB7.2. A cytotoxic monoclonal antibody with specificity for HLA-A2 and a variant of HLA-A28.},
  journal         = {Human immunology},
